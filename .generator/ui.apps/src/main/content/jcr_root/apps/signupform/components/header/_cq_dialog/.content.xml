<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Header"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
               <logoResType jcr:primaryType="nt:unstructured"
                            name="./logo/sling:resourceType"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            value="core/wcm/components/image/v2/image"/>
               <logo allowUpload="{Boolean}false"
                     autoStart="{Boolean}false"
                     class="cq-droptarget"
                     cq-msm-lockable="./logo/file"
                     fieldLabel="Logo"
                     fileNameParameter="./logo/fileName"
                     fileReferenceParameter="./logo/fileReference"
                     jcr:primaryType="nt:unstructured"
                     mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                     multiple="{Boolean}false"
                     name="./logo/file"
                     sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                     title="Drag to select image"
                     uploadUrl="${suffix.path}"
                     useHTML5="{Boolean}true"/>
               <aboutus cq-msm-lockable="./aboutus"
                        fieldDescription="Insert label"
                        fieldLabel="About Us"
                        jcr:primaryType="nt:unstructured"
                        name="./aboutus"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <aboutuspath cq-msm-lockable="./aboutuspath"
                            fieldDescription="Path to the about us page"
                            fieldLabel="Path"
                            jcr:primaryType="nt:unstructured"
                            name="./aboutuspath"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <products cq-msm-lockable="./products"
                         fieldDescription="Insert label"
                         fieldLabel="Products and Services"
                         jcr:primaryType="nt:unstructured"
                         name="./products"
                         sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <productspath cq-msm-lockable="./productspath"
                             fieldDescription="Path to the products page"
                             fieldLabel="Path"
                             jcr:primaryType="nt:unstructured"
                             name="./productspath"
                             sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <store cq-msm-lockable="./store"
                      fieldDescription="Insert label"
                      fieldLabel="Store"
                      jcr:primaryType="nt:unstructured"
                      name="./store"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <storepath cq-msm-lockable="./storepath"
                          fieldDescription="Path to the store page"
                          fieldLabel="Path"
                          jcr:primaryType="nt:unstructured"
                          name="./storepath"
                          sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <help cq-msm-lockable="./help"
                     fieldDescription="Insert label"
                     fieldLabel="Help"
                     jcr:primaryType="nt:unstructured"
                     name="./help"
                     sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <helppath cq-msm-lockable="./helppath"
                         fieldDescription="Path to the help page"
                         fieldLabel="Path"
                         jcr:primaryType="nt:unstructured"
                         name="./helppath"
                         sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <cartResType jcr:primaryType="nt:unstructured"
                            name="./cart/sling:resourceType"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            value="core/wcm/components/image/v2/image"/>
               <cart allowUpload="{Boolean}false"
                     autoStart="{Boolean}false"
                     class="cq-droptarget"
                     cq-msm-lockable="./cart/file"
                     fieldLabel="Cart"
                     fileNameParameter="./cart/fileName"
                     fileReferenceParameter="./cart/fileReference"
                     jcr:primaryType="nt:unstructured"
                     mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                     multiple="{Boolean}false"
                     name="./cart/file"
                     sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                     title="Drag to select image"
                     uploadUrl="${suffix.path}"
                     useHTML5="{Boolean}true"/>
               <searchResType jcr:primaryType="nt:unstructured"
                              name="./search/sling:resourceType"
                              sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                              value="core/wcm/components/image/v2/image"/>
               <search allowUpload="{Boolean}false"
                       autoStart="{Boolean}false"
                       class="cq-droptarget"
                       cq-msm-lockable="./search/file"
                       fieldLabel="search"
                       fileNameParameter="./search/fileName"
                       fileReferenceParameter="./search/fileReference"
                       jcr:primaryType="nt:unstructured"
                       mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                       multiple="{Boolean}false"
                       name="./search/file"
                       sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                       title="Drag to select image"
                       uploadUrl="${suffix.path}"
                       useHTML5="{Boolean}true"/>
            </items>
         </column>
      </items>
   </content>
</jcr:root>
